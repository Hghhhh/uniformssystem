  </resultMap>
  <insert id="insert" parameterType="Discount" >
    insert into tb_discount (discountCondition, reduce, discount, 
      start, end)
    values (#{discountcondition,jdbcType=INTEGER}, #{reduce,jdbcType=INTEGER}, #{discount,jdbcType=REAL}, 
      #{start,jdbcType=TIMESTAMP}, #{end,jdbcType=TIMESTAMP})
